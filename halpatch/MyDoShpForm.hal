procedure DoSh1Form(record RcVc RepSpec,record SHVc SHr)
BEGIN
  record Sh1DefBlock Sh1DefRec;
  row Sh1DefBlock Sh1Defrw;
  Integer i,rwcnt;
  Boolean printf,langf;
  Integer intdocnr;
  string 30 formcode,langcode,langcod2;
  record SHVc resSHr;
  record ModuleBlock MBr;

  BlockLoad(MBr);
  langcode = SHr.LangCode;
  langcod2 = SHr.ShipMode;
  BlockLoad(Sh1DefRec);
  rwcnt = MatRowCnt(Sh1DefRec);
  for (i=rwcnt-1;i>=0;i=i-1) begin
    MatRowGet(Sh1DefRec,i,Sh1Defrw);
    if (Sh1Defrw.LangCode==langcode) then begin
      i = -1;
      langf = true;
    end;
  end;
  if (langf==false) then begin langcode = ""; end;
  intdocnr = 1;
  printf = true;
  if (SHr.Prntdf!=0) then begin
    if (UserCanAction("DisallowPrintCopy",false)) then begin
      MessageBox(1274,StringFromStringSet(3,"DisallowPrintCopy"));
      printf = false;
    end;
  end;
  while (printf) begin  
LFINDFORMCOCESH1:;
    formcode = "";
    for (i=rwcnt-1;i>=0;i=i-1) begin
      MatRowGet(Sh1DefRec,i,Sh1Defrw);
      if (FindFormcode(SHr.OKFlag,Sh1Defrw.Typ)) then begin 
        if (Getformcode(i,Sh1Defrw.intdocnr,Sh1Defrw.FPCode,Sh1Defrw.UserGroup,Sh1Defrw.LangCode,Sh1Defrw.SerNr,SHr.SerNr,Sh1Defrw.PrintGroupCode,
                        langcode,intdocnr,"SHVc",formcode)) then
        begin
          goto LBREAKSH1;
        end;
      end;
    end;
    if (nonblank(langcod2)) then begin
      langcode = langcod2;
      langcod2 = "";
      goto LFINDFORMCOCESH1;
    end;  
LBREAKSH1:;
    if (nonblank(formcode)) then begin
      if (OpenForm(formcode)) then begin    
        if (MBr.NoTestprintEnabler==0) then begin
          if (SHr.OKFlag==0) then begin
            FormWaterMark(1117);
            printf = false;
          end;
          if (SHr.OKFlag!=0) then begin
            if (SHr.Prntdf!=0) then begin
              FormWaterMark(1118);
              printf = false;
            end;
          end;
        end;
        ExpandShipment(RepSpec.repname,RepSpec.flags[1],SHr,resSHr);
//        SummarizeComponents(resSHr);,setting
        PrintShipment(RepSpec,resSHr,formcode);
        NumberofDocumentPages(Sh1Defrw.FPCode,MatRowCnt(resSHr));
        CloseForm;
      end else begin
        printf = false;
        MessageBox(1546,formcode);
      end;
    end else begin
      printf = false;
      if (intdocnr==1) then begin
        MessageBox(1624, " " & USetStr(1623));
      end;
    end;
    intdocnr = intdocnr + 1;
  end;
  RETURN;
END;

