procedure ALPB2ResGr6(record RcVc RepSpec,Boolean PLf,record ResGr6Vc ResGr6p,record AccVc Accp,string anru,
                var val S3bp,var val Pbp,var val Ebp,var val S4bp,var val S5bp,var val S6bp,
                Boolean addflag,val goms,var Boolean headflagp,Boolean resUseAccum,Date AccumStartDate)
BEGIN
  Boolean negf,usedf;
  val S3b1,Pb1,Eb1,S4b1,S5b1,S6b1;
  string 255 nru;
  record CYBlock CompYear;
  string 255 tstr,tstr2;
  Date budsd,buded;
  
  budsd = StringToDate(FirstInRange(RepSpec.Stext,10));
  if (blankdate(budsd)) then begin budsd = RepSpec.sStartDate; end;    
  buded = StringToDate(LastInRange(RepSpec.Stext,10));
  if (blankdate(buded)) then begin buded = RepSpec.sEndDate; end;    
//  if (AccBlockedTest((AccessView*)appSlotArr[gCurSlot].ast_Accessp,Accp->AccNumber)) then goto LALPB2;
  BlockLoad(CompYear);
  nru = anru;//??
  RepSpec.f5 = nru;// Edit ************************** Tuesday, 17 May 2011 10:47:48
  // Edit Start ---------------------------------------------- Edit Start
	//Wednesday, 27 April 2011 10:41:07
	if(nonblank(RepSpec.ObjStr))then begin
    nru = RepSpec.ObjStr;//??
    RepSpec.f5 = nru;// Edit ************************** Tuesday, 17 May 2011 10:47:48
  end;
	// Edit End ---------------------------------------------- Edit End
  usedf = AccRuPerBal(RepSpec,PLf,Accp.AccNumber,nru,S3b1,Pb1,Eb1,S4b1,S5b1,S6b1,ResGr6p.DCSide,resUseAccum,AccumStartDate,budsd,buded);
/*  
  if (ResGr6p.DCSaldo==1) then begin
    if (Eb1<0) then begin goto LALPB2ResGr6; end;
  end;
  if (ResGr6p.DCSaldo==2) then begin
    if (Eb1>=0) then begin goto LALPB2ResGr6; end;
  end;
*/  
  switch (RepSpec.Comparison) begin
    case 0:
      if (ResGr6p.DCSaldo==1) then begin
        if (Eb1<0) then begin goto LALPB2ResGr6; end;//S3b1
      end;
      if (ResGr6p.DCSaldo==2) then begin
        if (Eb1>0) then begin goto LALPB2ResGr6; end;//S3b1
      end;
      if (Eb1==0) then begin
        if (ResGr6p.DCSaldo==1) then begin
          if (Pb1>0) then begin goto LALPB2ResGr6; end;
        end;
        if (ResGr6p.DCSaldo==2) then begin
          if (Pb1<0) then begin goto LALPB2ResGr6; end;
        end;
      end;            
    otherwise
      if (ResGr6p.DCSaldo==1) then begin
        if (Pb1<0) then begin goto LALPB2ResGr6; end;
      end;
      if (ResGr6p.DCSaldo==2) then begin
        if (Pb1>=0) then begin goto LALPB2ResGr6; end;
      end;
  end;              
  GetResGr6Description(ResGr6p,RepSpec.Language,tstr2);  
  if (nonblank(tstr2)) then begin// ResGr6p->Comment
    if ((((RepSpec.AccSpec==1) or (RepSpec.AccSpec==4)) and ((Pb1!=0) or (S3b1!=0) or (S4b1!=0) or (S5b1!=0) or (S6b1!=0) or(Eb1!=0) or (usedf))) 
        or (RepSpec.AccSpec==0)) then begin//or (nonblank(RepSpec.ObjType))
      if (headflagp==false) then begin
        StartFormat(15);
        OutString(0,0,tstr2,false); 
        EndFormat;
        headflagp = true;
        Gray_Divider(0,1);
      end;
      negf = true;
      if (Accp.AccType==0) then begin
        if (CompYear.NS0[0]==0) then begin
          negf = false;
        end;
      end;
      if (Accp.AccType==1) then begin
        if (CompYear.NS0[1]==0) then begin
          negf = false;
        end;
      end;
      if (Accp.AccType==2) then begin
        if (CompYear.NS0[2]==0) then begin
          negf = false;
        end;
      end;
      if (Accp.AccType==3) then begin
        if (CompYear.NS0[3]==0) then begin
          negf = false;
        end;
      end;
      if (Accp.AccType==4) then begin
        if (CompYear.NS0[4]==0) then begin
          negf = false;
        end;
      end;                    
      GetAccountDescription2(Accp.AccNumber,RepSpec.Language,tstr);            
      if (blank(RepSpec.ObjType)) then begin
        AGrSumLine(RepSpec,0,Accp.AccNumber,tstr,ResGr6p.SpecCode,S3b1,Pb1,Eb1,S4b1,S5b1,S6b1,negf,goms,PLf,resUseAccum,"DblNL",6,ResGr6p.IdKey,usedf);
      end else begin
        SumLine2(RepSpec,PLf,Accp.AccNumber,tstr,ResGr6p.SpecCode,S3b1,Pb1,Eb1,S4b1,S5b1,S6b1,negf,goms,resUseAccum,AccumStartDate,ResGr6p.DCSide,6,ResGr6p.IdKey);
      end;      
    end;
  end;
  if (addflag==true) then begin
    S3bp = S3bp + S3b1;
    S4bp = S4bp + S4b1;
    S5bp = S5bp + S5b1;
    S6bp = S6bp + S6b1;
    Pbp = Pbp + Pb1;
    Ebp = Ebp + Eb1;
  end else begin
    S3bp = S3bp - S3b1;
    S4bp = S4bp - S4b1;
    S5bp = S5bp - S5b1;
    S6bp = S6bp - S6b1;
    Pbp = Pbp - Pb1;
    Ebp = Ebp - Eb1;
  end;
LALPB2ResGr6:;
  RETURN;
END;

procedure ALPBEnd(record RcVc RepSpec,Boolean PLf,string Comment,string extracode,val S3bp,val Pbp,val Ebp,
                  val S4bp,val S5bp,val S6bp,Boolean negf,val goms,Boolean aheadflag,Boolean resUseAccum,LongInt idkey)
BEGIN
  string 255 tstr;
  
  if ((RepSpec.AccSpec==0) or (RepSpec.AccSpec==1) or (RepSpec.AccSpec==4)) then begin
    if (aheadflag==true) then begin
      tstr = USetStr(3143) & Comment;
      if (nonblank(Comment)) then begin
        Gray_Divider(0,1);
        AGrSumLine(RepSpec,0,tstr,"",extracode,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,negf,goms,PLf,resUseAccum,"",6,idkey,false);
        StartFormat(15);
        EndFormat;
      end;
    end;
  end;
  if (RepSpec.AccSpec==2) then begin
    if (nonblank(Comment)) then begin
      AGrSumLine(RepSpec,0,Comment,"",extracode,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,negf,goms,PLf,resUseAccum,"",6,idkey,false);
    end;
  end;
  if (RepSpec.AccSpec==3) then begin
    if ((S3bp!=0) or (S4bp!=0) or (S5bp!=0) or (S6bp!=0) or (Pbp!=0)) then begin
      if (nonblank(Comment)) then begin
        AGrSumLine(RepSpec,0,Comment,"",extracode,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,negf,goms,PLf,resUseAccum,"",6,idkey,false);
      end;
    end;
  end;
  RETURN;
END;


procedure AccListPerBalResGr6(record RcVc RepSpec,Boolean PLf,record ResGr6Vc ResGr6p,var val S3bp,var val Pbp,
                        var val Ebp,var val S4bp,var val S5bp,var val S6bp,Boolean negf,val goms,Boolean resUseAccum,
                        Date AccumStartDate)
BEGIN
  record AccVc Accr;
  Boolean addflag;
  string 255 Nitem;
  string 255 Nitem2;
  string 255 NRu;
  string 5 nsep;
  Boolean Accs;
  Integer np;
  Boolean headflag;
  string 255 tstr;

  S3bp = 0;
  S4bp = 0;
  S5bp = 0;
  S6bp = 0;
  Pbp = 0;
  Ebp = 0;
  headflag = false;
  np = 0;
  GetNext2Sep(ResGr6p.Desc,np,nsep);
  while (GetNextTokenReturn(ResGr6p.Desc,np,Nitem)) begin
    if (nsep=="-") then begin
      addflag = false; 
    end else begin
      addflag = true;
    end;
    GetNext2Sep(ResGr6p.Desc,np,nsep);
    if (nsep==".") then begin
      GetNextToken(ResGr6p.Desc,np,NRu);
      GetNext2Sep(ResGr6p.Desc,np,nsep);
    end else begin
      NRu = "";
    end;    
    if (nsep==":") then begin
      GetNextToken(ResGr6p.Desc,np,Nitem2);
      GetNext2Sep(ResGr6p.Desc,np,nsep);
    end else begin
      Nitem2 = "";
    end;    
    Accr.AccNumber = Nitem;
    if (blank(Nitem2)) then begin
      Accs = ReadFirstMain(Accr,1,true);
      if ((Accs) and (Accr.GroupAcc==0)) then begin
        ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
      end;
    end else begin
      if (Nitem<Nitem2) then begin
        ResetLoop(Accr);
        Accs = true;
        while (LoopMain(Accr,2,Accs)) begin
          if (Accs==true) then begin
            if (Accr.AccNumber>Nitem2) then begin Accs = false; end;
          end;
          if ((Accs) and (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;
        end;
      end else begin
        ResetLoop(Accr);
        Accr.AccNumber = Nitem;
        Accs = LoopBackKey("AccNumber",Accr,1,true);
        while (Accs) begin
          if (Accs==true) then begin
            if (Accr.AccNumber<Nitem2) then begin Accs = false; end;
          end;  
          if ((Accs) and (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;  
          Accs = LoopBackKey("AccNumber",Accr,1,true);//Accs = m4_ReadLogicalMain(AccVc,LLoop1--,&Accr);
        end;
      end;
    end;
  end;
  np = 0;
  GetNext2Sep(ResGr6p.Desc2,np,nsep);
  while (GetNextTokenReturn(ResGr6p.Desc2,np,Nitem)) begin
    if (nsep=="-") then begin
      addflag = false; 
    end else begin
      addflag = true;
    end;  
    GetNext2Sep(ResGr6p.Desc2,np,nsep);
    if (nsep==".") then begin
      GetNextToken(ResGr6p.Desc2,np,NRu);
      GetNext2Sep(ResGr6p.Desc2,np,nsep);
    end else begin
      NRu = "";
    end;    
    if (nsep==":") then begin
      GetNextToken(ResGr6p.Desc2,np,Nitem2);
      GetNext2Sep(ResGr6p.Desc2,np,nsep);
    end else begin
      Nitem2 = "";
    end;    
    Accr.AccNumber = Nitem;
    if (blank(Nitem2)) then begin
      Accs = ReadFirstMain(Accr,1,true);
      if ((Accs) and  (Accr.GroupAcc==0)) then begin
        ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
      end;    
    end else begin
      if (Nitem<Nitem2) then begin
        ResetLoop(Accr);
        Accs = true;
        while (LoopMain(Accr,2,Accs)) begin
          if (Accs==true) then begin
            if (Accr.AccNumber>Nitem2) then begin Accs = false; end;
          end;  
          if ((Accs) and  (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;
        end;
      end else begin
        ResetLoop(Accr);
        Accr.AccNumber = Nitem;
        Accs = LoopBackKey("AccNumber",Accr,1,true);
        while (Accs) begin
          if (Accs==true) then begin
            if (Accr.AccNumber<Nitem2) then begin Accs = false; end;
          end;  
          if ((Accs) and  (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;  
          Accs = LoopBackKey("AccNumber",Accr,1,true);//Accs = m4_ReadLogicalMain(AccVc,LLoop1--,&Accr);
        end;
      end;
    end;
  end;
  np = 0;
  GetNext2Sep(ResGr6p.Desc3,np,nsep);
  while (GetNextTokenReturn(ResGr6p.Desc3,np,Nitem)) begin
    if (nsep=="-") then begin
      addflag = false; 
    end else begin
      addflag = true;
    end;  
    GetNext2Sep(ResGr6p.Desc3,np,nsep);
    if (nsep==".") then begin
      GetNextToken(ResGr6p.Desc3,np,NRu);
      GetNext2Sep(ResGr6p.Desc3,np,nsep);
    end else begin
      NRu = "";
    end;    
    if (nsep==":") then begin
      GetNextToken(ResGr6p.Desc3,np,Nitem2);
      GetNext2Sep(ResGr6p.Desc3,np,nsep);
    end else begin
      Nitem2 = "";
    end;    
    Accr.AccNumber = Nitem;
    if (blank(Nitem2)) then begin
      Accs = ReadFirstMain(Accr,1,true);
      if ((Accs) and  (Accr.GroupAcc==0)) then begin
        ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
      end;
    end else begin
      if (Nitem<Nitem2) then begin
        ResetLoop(Accr);
        Accs = true;
        while (LoopMain(Accr,2,Accs)) begin
          if (Accs==true) then begin
            if (Accr.AccNumber>Nitem2) then begin Accs = false; end;
          end;  
          if ((Accs) and  (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;  
        end;
      end else begin
        ResetLoop(Accr);
        Accr.AccNumber = Nitem;
        Accs = LoopBackKey("AccNumber",Accr,1,true);
        while (Accs) begin
          if (Accs==true) then begin
            if (Accr.AccNumber<Nitem2) then begin
              Accs = false;
            end;  
          end;  
          if ((Accs) and  (Accr.GroupAcc==0)) then begin
            ALPB2ResGr6(RepSpec,PLf,ResGr6p,Accr,NRu,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,addflag,goms,headflag,resUseAccum,AccumStartDate);
          end;
          Accs = LoopBackKey("AccNumber",Accr,1,true);
        end;
      end;
    end;
  end;
  GetResGr6Description(ResGr6p,RepSpec.Language,tstr);
  ALPBEnd(RepSpec,PLf,tstr,ResGr6p.SpecCode,S3bp,Pbp,Ebp,S4bp,S5bp,S6bp,negf,goms,headflag,resUseAccum,ResGr6p.IdKey);// ResGr6p.Comment
  RETURN;
END;

