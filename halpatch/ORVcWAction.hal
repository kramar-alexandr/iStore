      chsum = ORDchrsum(ORr,rownr);
    end;
    if (chsum) then begin
      ORDchsum(ORr,rownr);
      ORSumup(ORr);
    end;    
    PutWindowRecord(wn,ORr);
  end;
  ORDClassQuantEFAfter = true;
  return;
end;
 
