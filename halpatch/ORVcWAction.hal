begin   
  record ORVc ORr;
  Boolean chsum;
  record ModuleBlock OptFeature;

  if (changedf!=0) then begin 
    GetWindowRecord(wn,ORr);
    DeselectWindow(wn,false);
    BlockLoad(OptFeature);
