  string 30 formcode,langcode,langcod2;
  record SHVc resSHr;

  langcode = SHr.LangCode;
  langcod2 = SHr.ShipMode;
  BlockLoad(Sh3DefRec);
  rwcnt = MatRowCnt(Sh3DefRec);
  for (i=rwcnt-1;i>=0;i=i-1) begin
    MatRowGet(Sh3DefRec,i,Sh3Defrw);
    if (Sh3Defrw.LangCode==langcode) then begin
      i = -1;
      langf = true;
    end;
  end;
  if (langf==false) then begin langcode = ""; end;
  intdocnr = 1;
  printf = true;
  while (printf) begin  
LFINDFORMCOCESH3:;
    formcode = "";
    for (i=rwcnt-1;i>=0;i=i-1) begin
      MatRowGet(Sh3DefRec,i,Sh3Defrw);
      if (FindFormcode(SHr.OKFlag,Sh3Defrw.Typ)) then begin 
        if (Getformcode(i,Sh3Defrw.intdocnr,Sh3Defrw.FPCode,Sh3Defrw.UserGroup,Sh3Defrw.LangCode,Sh3Defrw.SerNr,SHr.SerNr,Sh3Defrw.PrintGroupCode,
                        langcode,intdocnr,"SHVc",formcode)) then
        begin
          goto LBREAKSH3;
        end;
      end;
    end;
    if (nonblank(langcod2)) then begin
      langcode = langcod2;
      langcod2 = "";
      goto LFINDFORMCOCESH3;
    end;  
LBREAKSH3:;
    if (nonblank(formcode)) then begin
      if (OpenForm(formcode)) then begin  
          ExpandShipment(RepSpec.repname,RepSpec.flags[1],SHr,resSHr);
//          SummarizeComponents(resSHr);//setting
          PrintShipment(RepSpec,resSHr,formcode);
          NumberofDocumentPages(Sh3Defrw.FPCode,MatRowCnt(resSHr));
          CloseForm;
      end else begin
        printf = false;
        MessageBox(1546,formcode);
      end;
    end else begin
      printf = false;
      if (intdocnr==1) then begin
        MessageBox(1624, " " & USetStr(1623));
      end;
    end;
    intdocnr = intdocnr + 1;
  end;
  RETURN;
END;

