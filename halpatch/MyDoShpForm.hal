//      OUTFORMFIELD("F_ORDANTAL",ValToString(StringToVal(SHrw.Ordered,M4UVal),M4UVal,SysFormatRec.thousSep,SysFormatRec.decimalPt,LangNrr.CutDecimals));
      t = SHrw.Ordered - SHrw.Ship;
