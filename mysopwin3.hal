function boolean CheckMyMachine()begin record CashVc Cashr;  record LocalMachineBlock LMb;  integer wn;  boolean res;    res = false;  wn = curwindow;  getWindowRecord(wn,Cashr);  blockload(LMb);  if ((Cashr.MachineName == LMb.LocalMachineCode) or blank(Cashr.MachineName)) then begin   res = true;  end;  CheckMyMachine = res;return;end;event DefineWindowsbegin  real h,h1,hl,h2,h3,h4,h5,h6,h7,hs,v,vm,l,vs,f,v2,t,v3;  string 255 label;  integer flagtr;     SetLangMode(LangUkrainian,"UKR",0);     WindowBegin("Події ЕККА",CashLClass,CGmlist,-);  //SpawnClass(CashDClass);  SpawnClass(CashINOUTTClass);    SetWRect(64,80,509,298);  Sizeable(-1,-1,0,0);  UseView(CashVc);  RecordList(4,17,-21,-39);  RlColKey(4,4,"№",SerNr,false,SerNr);  RlCol(104,104,"OK",OKFlag,false);  RlColKey(134,134,"Дата операції",TransDate,false,TransDate);  RlColKey(214,214,"Скринька",Drawer,false,Drawer);  RlCol(274,274,"Машина",MachineName,false);  //RlCol(-80,-80,"Разом",Total,true);  RlCol(-1,-1,"",Invalid,false);  SearchField("Пошук");  EndWindow;    WindowBegin("Подія ЕККА",CashDClass,CGview,-);    SetWRect(64,80,590,400);    BrowseButtons(false);    DynamicWindow;    UseView(CashVc);    if(CheckMyMachine) then begin                        ActiveFieldOnNew("Скринька");        vs = 20; h = 100; h2 = 260; h3 = 350;        EditField(h,v=6,70,"Номер",Normal,SerNr,false,0);        EditField(h2,vm=v,70,"Знак",ViewOnly,Sign,false,0);        EditField(h,v=v+vs,70,"Скринька",Normal,Drawer,false,DrawerSClass);        EditField(h2,vm=v,70,"Машина",Normal,MachineName,false,LocalMachineSClass);        EditField(h,v=v+vs,70,"Дата",Normal,TransDate,false,PasteCurDate);        EditField(h2,v,70,"Час",Normal,TransTime,false,PasteCurTime);        EditField(h,v3=(v=v+vs),-40,"Коментар",Normal,Comment,false,0);        ButtonFrame(h3,v=12,123,2,"Подія");        RadioButton(h3,v,0,0,"Вилучення готівки",Event);         RadioButton(h3,v+=16,1,0,"Поповнення готівки",Event);        RadioButton(h3,v+=16,2,0,"Списання готівки",Event);        MatrixBegin(h,v=v3+2*vs,h+150,v+10*17+1,-,99);        MatCol(1,24,"Вид",0,PMCode,false,PMSClass);        MatCol(1,70,"Сума",0,Amount,true,0);        InsertRowTest;        DeleteRowTest;        EndMatrix;        EditField(h+70,-30,80,"Разом",Normal,Total,true,0);        CheckBox(20,-20,0,"OK",OKFlag);                    end else begin      StaticText(100,120,"Ви не можете переглядати чужу подію ЕККА",false);    end;   EndWindow;       /* WindowBegin("Внесення готівки",CashINOUTTClass,CGother,RcType);			SetWRect(90,80,500,200);			EditField(50,20,-40,"Сумма",Normal,vals0,false,0);			ColourPushButton(10,46,90,96,"MyCashInMn","Внесення",2);			ColourPushButton(-90,46,-10,96,"MyCashOutMn","Вилучення",1);		EndWindow;*/       return;end;